{
	"name": "pokeapi",
	"version": "0.1.0",
	"private": true,
	"description": "Making a Pokemon API project to learn React and GraphQL",
	"dependencies": {
		"@apollo/client": "^3.4.11",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^11.2.7",
		"@testing-library/user-event": "^12.8.3",
		"axios": "^0.21.4",
		"bootstrap": "^5.1.1",
		"graphql": "^15.5.3",
		"mdbreact": "^5.1.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-dropdown": "^1.9.2",
		"react-icons": "^4.2.0",
		"react-router-dom": "^5.3.0",
		"react-scripts": "4.0.3",
		"styled-components": "^5.3.1",
		"web-vitals": "^1.1.2"
	},
	"scripts": {
		"cypress:gui_all_prod": "cypress open --env configFile=prod",
		"cypress:gui_all_local": "cypress open --env configFile=local",
		"cypress:all_local": "cypress run --env configFile=local",
		"cypress:gui_smoke_local": "cypress open --config ignoreTestFiles=!**/*/smoke/**/* --env configFile=local",
		"cypress:smoke_local": "cypress run --config ignoreTestFiles=!**/*/smoke/**/* --env configFile=local",
		"cypress:gui_p1_local": "cypress open --config ignoreTestFiles=!**/*/p1/**/* --env configFile=local",
		"cypress:p1_local": "cypress run --config ignoreTestFiles=!**/*/p1/**/* --env configFile=local",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"cypress": "^8.5.0",
		"eslint": "^7.32.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-react": "^7.26.1"
	}
}
